<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginVerification</servlet-name>
    <servlet-class>com.Login.LoginVerification</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>stuSearch</servlet-name>
    <servlet-class>com.stuManager.stuSearch</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>test</servlet-name>
    <servlet-class>com.stuManager.test</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>checkStuNo</servlet-name>
    <servlet-class>com.stuManager.checkStuNo</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>checkDromSlot</servlet-name>
    <servlet-class>DromManager.checkDromSlot</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>stuAdd</servlet-name>
    <servlet-class>com.stuManager.stuAdd</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>LoginVerification</servlet-name>
    <url-pattern>/servlet/LoginVerification</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>stuSearch</servlet-name>
    <url-pattern>/servlet/stuSearch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/servlet/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>checkStuNo</servlet-name>
    <url-pattern>/servlet/checkStuNo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>checkDromSlot</servlet-name>
    <url-pattern>/servlet/checkDromSlot</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>stuAdd</servlet-name>
    <url-pattern>/servlet/stuAdd</url-pattern>
  </servlet-mapping>
</web-app>