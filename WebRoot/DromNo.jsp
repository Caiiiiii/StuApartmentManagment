<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%String path = request.getContextPath();String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html>  <head>    <base href="<%=basePath%>">        <title>My JSP 'DromNo.jsp' starting page</title>    	<meta http-equiv="pragma" content="no-cache">	<meta http-equiv="cache-control" content="no-cache">	<meta http-equiv="expires" content="0">    	<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">	<meta http-equiv="description" content="This is my page">	<link href="css/bootstrap.min.css" rel="stylesheet">	<script src="http://code.jquery.com/jquery-latest.js"></script>	<!--	<link rel="stylesheet" type="text/css" href="styles.css">	-->  </head>    <body>  		<div class="form-size">	    	<form>	    		<div class="form-group has-feedback" id="dromNoDiv">	    			<label for="inputBuilding">宿舍楼</label>	    			<input type="text" name="inputBuilding" class="form-control" id="inputBuilding">	    			<span class=" glyphicon glyphicon-ok form-control-feedback display-point" id="NoSuccess"></span>    				<span class=" glyphicon glyphicon-remove form-control-feedback display-point" id="NoFalse"></span>	    		</div>	    		<div class="form-group has-feedback" id="floorDiv">	    			<label for="inputFloor">楼层</label>	    			<input type="text" name="inputFloor" class="form-control" id="inputFloor">	    			<span class=" glyphicon glyphicon-ok form-control-feedback display-point" id="floorSuccess"></span>    				<span class=" glyphicon glyphicon-remove form-control-feedback display-point" id="floorFalse"></span>	    		</div>	    		<div class="form-group has-feedback" id="roomDiv">	    			<label for="inputRoom">房间号</label>	    			<input type="text" name="inputRoom" class="form-control" id="inputRoom">	    			<span class=" glyphicon glyphicon-ok form-control-feedback display-point" id="roomSuccess"></span>    				<span class=" glyphicon glyphicon-remove form-control-feedback display-point" id="roomFalse"></span>	    		</div>	    		<div class="btu-size">	    			<input type="button" id="inputbutton" class="btn btn-primary  btn-block" value="添加">	    		</div>	    	</form>    	</div>  </body>  <style type="text/css">  	.form-size{  		width: 400px;  	}  	.btu-size{  		position:relative;  		left: 25%;  		width: 50%;  	}  	.display-point{  		display: none;  	}  </style>  <script type="text/javascript">  	$(document).ready(function () {  			var building = true;	$("#inputBuilding").blur(function(){  			if($("#inputBuilding").val().length == 1){  				var regExp = /[A-Z]$/;  				if(!regExp.test($(this).val())){  						building = false;  						$("#dromNoDiv").removeClass("has-success");  						$("#dromNoDiv").addClass("has-error");  						$("#NoSuccess").css("display","none");  						$("#NoFalse").css("display","block");  				}else{  						building = true;  						$("#dromNoDiv").removeClass("has-error");  						$("#dromNoDiv").addClass("has-success");  						$("#NoSuccess").css("display","block");  						$("#NoFalse").css("display","none");  				}  			}  			else{  						building = false;  						$("#dromNoDiv").removeClass("has-success");  						$("#dromNoDiv").addClass("has-error");  						$("#NoSuccess").css("display","none");  						$("#NoFalse").css("display","block");  			}  		});		var floor = true;	$("#inputFloor").blur(function(){  			if($("#inputFloor").val().length<=2 &&  $("#inputFloor").val().length >0){  				var regExp = /[0-9]$/;  				if(!regExp.test($(this).val())){  						floor = false;  						$("#floorDiv").removeClass("has-success");  						$("#floorDiv").addClass("has-error");  						$("#floorSuccess").css("display","none");  						$("#floorFalse").css("display","block");  				}else{  						floor = true;  						$("#floorDiv").removeClass("has-error");  						$("#floorDiv").addClass("has-success");  						$("#floorSuccess").css("display","block");  						$("#floorFalse").css("display","none");  				}  			}  			else{  						floor = false;  						$("#floorDiv").removeClass("has-success");  						$("#floorDiv").addClass("has-error");  						$("#floorSuccess").css("display","none");  						$("#floorFalse").css("display","block");  			}  		});	var room = true;	$("#inputRoom").blur(function(){  			if($("#inputRoom").val().length<=2 && $("#inputRoom").val().length >0){  				var regExp = /[0-9]$/;  				if(!regExp.test($(this).val())){  						room = false;  						$("#roomDiv").removeClass("has-success");  						$("#roomDiv").addClass("has-error");  						$("#roomSuccess").css("display","none");  						$("#roomFalse").css("display","block");  				}else{  						room = true;  						$("#roomDiv").removeClass("has-error");  						$("#roomDiv").addClass("has-success");  						$("#roomSuccess").css("display","block");  						$("#roomFalse").css("display","none");  				}  			}  			else{  						room = false;  						$("#roomDiv").removeClass("has-success");  						$("#roomDiv").addClass("has-error");  						$("#roomSuccess").css("display","none");  						$("#roomFalse").css("display","block");  			}  		});	$("#inputbutton").click(function(){				if(building==true&&floor==true&&room==true){					var DromNo = $("#inputBuilding").val()+$("#inputFloor").val()+$("#inputRoom").val();					$.ajax({						type:'GET',						url:'servlet/checkDromNo?inputDromNo='+DromNo+'&Building='+$("#inputBuilding").val()+'&Floor='+$("#inputFloor").val()+'&Room='+$("#inputRoom").val(),						success:function(data){							if(data == "1"){								alert("success");							}							else if(data == "2"){								alert("false");							}						}					});				}else{					alert("输入信息有误");			}		});  	});  </script></html>